{
    "apps": {
        "tls": {
            "certificates": {
                "automate": [
                    "hk.zhurongshuo.com"
                ]
            },
            "automation": {
                "policies": [
                    {
                        "issuers": [
                            {
                                "module": "acme",
                                "challenges": {
                                    "dns": {
                                        "provider": {
                                            "name": "alidns",
                                            "access_key_id": "LTAI5t93TNStUVvYVJq2kqSW",
                                            "access_key_secret": "MVlzb9bw7IjLcJDlRvDsehsrZGU9Uy"
                                        }
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        },
        "http": {
            "servers": {
                "hugo": {
                    "listen": [
                        ":443"
                    ],
                    "automatic_https": {
                        "disable": false
                    },
                    "routes": [
                        {
                            "handle": [
                                {
                                    "handler": "file_server",
                                    "root": "/docs"
                                }
                            ],
                            "match": [
                              {
                                "not": [
